{
  "name": "hofladen",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint .",
    "db:first-initialization": "npm run db:start && npx supabase db pull && npx supabase migration up && npm run db:reset",
    "db:generate-types": "npx supabase gen types typescript --local > src/types/database.types.ts",
    "db:dump": "npx supabase db dump --data-only --schema public --file supabase/seed.sql",
    "db:reset": "npx supabase db reset",
    "db:dump-reset": "npm run db:dump && npm run db:reset",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop"
  },
  "dependencies": {
    "@ag-grid-community/locale": "^34.0.2",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "7.4.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@ionic/vue": "^8.6.5",
    "@ionic/vue-router": "^8.6.5",
    "@supabase/supabase-js": "^2.52.0",
    "ag-grid-community": "^34.0.2",
    "ag-grid-vue3": "^34.0.2",
    "ionicons": "^8.0.13",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.2",
    "@vitejs/plugin-legacy": "^7.0.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.5.2",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "supabase": "^2.31.8",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "~7.0.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.3"
  },
  "description": "An Ionic project"
}
