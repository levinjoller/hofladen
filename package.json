{
  "name": "hofladen",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint .",
    "update-dependencies": "ncu -u",
    "reinstall-dependencies": "rd /s /q node_modules && npm cache clean --force && npm install",
    "db:first-initialization": "npm run db:start && npx supabase db pull && npx supabase migration up && npm run db:reset && npm run db:generate-types",
    "db:generate-types-and-zod": "npx tsx ./scripts/schema-generator.ts && npx tsx ./scripts/post-schema-generator.ts",
    "db:generate-types": "npx supabase gen types typescript --local > src/types/database.types.ts",
    "db:dump": "npx supabase db dump --data-only --schema public --file supabase/seed.sql",
    "db:reset": "npx supabase db reset && npm run db:generate-types && npm run db:generate-types-and-zod",
    "db:dump-reset": "npm run db:dump && npm run db:reset",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop"
  },
  "dependencies": {
    "@ag-grid-community/locale": "^34.1.1",
    "@capacitor/app": "7.0.2",
    "@capacitor/core": "7.4.2",
    "@capacitor/haptics": "7.0.2",
    "@capacitor/keyboard": "7.0.2",
    "@capacitor/status-bar": "7.0.2",
    "@ionic/vue": "^8.7.2",
    "@ionic/vue-router": "^8.7.2",
    "@supabase/supabase-js": "^2.55.0",
    "ag-grid-community": "^34.1.1",
    "ag-grid-vue3": "^34.1.1",
    "dotenv": "^17.2.1",
    "ionicons": "^8.0.13",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.5.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.2",
    "@types/pg": "^8.15.5",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.5.4",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "pg": "^8.16.3",
    "supabase": "^2.34.3",
    "terser": "^5.43.1",
    "tsx": "^4.20.3",
    "typescript": "~5.9.2",
    "vite": "~7.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "description": "An Ionic project"
}
